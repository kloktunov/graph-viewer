    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0",
    "file-saver": "^2.0.2",
    "node-sass": "4.13.0",
    "push-dir": "^0.4.1",
    "sass-loader": "8.0.0",
    "vue-observe-visibility": "^0.4.6"
  }
